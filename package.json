{
  "name": "workmate-testing-agent",
  "version": "1.0.0",
  "description": "LangGraph-based multi-agent testing system for Workmate AI",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "tsx src/index.ts",
    "test": "tsx src/index.ts --scenario full-lifecycle",
    "test:quick": "tsx src/index.ts --scenario quick-smoke",
    "test:homeowner": "tsx src/index.ts --scenario homeowner-flow",
    "test:tradesperson": "tsx src/index.ts --scenario tradesperson-flow",
    "clean-users": "tsx src/scripts/cleanup-test-users.ts",
    "dev": "tsx watch src/index.ts"
  },
  "dependencies": {
    "@langchain/core": "^0.3.0",
    "@langchain/langgraph": "^0.2.0",
    "@supabase/supabase-js": "^2.45.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "openai": "^4.70.0",
    "uuid": "^10.0.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/uuid": "^10.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0"
  }
}
